<style>
    #snackbar {
      visibility: hidden;
      min-width: 250px;
      margin-left: -125px;
      background-color: #333;
      color: #fff;
      text-align: center;
      border-radius: 2px;
      padding: 16px;
      position: fixed;
      z-index: 1;
      left: 50%;
      bottom: 30px;
      font-size: 17px;
    }
    
    #snackbar.show {
      visibility: visible;
      -webkit-animation: fadein 0.5s, fadeout 0.5s 2.5s;
      animation: fadein 0.5s, fadeout 0.5s 2.5s;
    }
    
    @-webkit-keyframes fadein {
      from {bottom: 0; opacity: 0;} 
      to {bottom: 30px; opacity: 1;}
    }
    
    @keyframes fadein {
      from {bottom: 0; opacity: 0;}
      to {bottom: 30px; opacity: 1;}
    }
    
    @-webkit-keyframes fadeout {
      from {bottom: 30px; opacity: 1;} 
      to {bottom: 0; opacity: 0;}
    }
    
    @keyframes fadeout {
      from {bottom: 30px; opacity: 1;}
      to {bottom: 0; opacity: 0;}
    }
    </style>
  </head>

  <body v-cloak>
    <section id="app" class="main">
      <h1>Gift Registry</h1>
      <div v-if="!showGifts" class="container">
        <div class="sign-up-content">
          <form novalidate @submit.prevent="registerGift" class="signup-form">
            <h2 class="form-title">Register your gift</h2>

            <div class="form-textbox">
              <label for="name">Full name</label>
              <input type="text" name="name" id="name" v-model="form.fullName">
            </div>
            <div class="form-textbox">
              <label for="giftType">Gift Name</label>
              <input type="text" name="giftType" id="giftType" v-model="form.giftName">
            </div>
            <div class="form-textbox">
              <label for="url">Item Url</label>
              <input type="text" name="url" id="url" v-model="form.giftUrl">
            </div>
            <div class="form-group">
              <input type="checkbox" name="agree-term" id="agree-term" v-model="form.isAnonymous" class="agree-term">
              <label for="agree-term" class="label-agree-term"><span><span></span></span>Register me as anonymous participant</label>
            </div>
            <div class="form-textbox">
              <a href="#" class="term-service" @click.prevent="showGifts=!showGifts">Show All Registered Gifts</a><input type="submit" name="submit" id="submit" class="submit" value="Register my Gift">
			</div>
			<div v-if="!errors.isValid && isSubmitted" class="o-Form-errors">Please enter your Fullname and the Gift Name.</div>
          </form>
          <div id="snackbar">Gift Registered Successfully.</div>
        </div>
	  </div>
	  
	  <!-- Show all Gifts -->
	  <div v-if="showGifts"  class="c-Show-gifts">
        <a href="#" class="term-service float-top" @click.prevent="showGifts=!showGifts">Back To Gift Form</a>
		  <div class="gifts-wrapper">
			  
				<article class="card" v-for="(gift, index) in allGifts" :key="gift.id">
          <div class="thumb" v-if="gift.imageUrl" :style="{ backgroundImage: `url(${gift.imageUrl})` }"></div>
          <div class="thumb" v-else style="background-size:100px;"></div>
					<div class="infos">
						<h2 class="title">{{gift.giftName}}
						<h3 class="seats">{{gift.fullName}}</h3>
						<p class="txt">{{gift.giftDescription}}</p>
					</div>
				</article>
		  </div>
	  </div>
    </section>

    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue-resource@1.5.1"></script>
    <script>
      var app = new Vue({
        el: "#app",
        data: {
			form: {
				fullName: '',
				giftName: '',
				giftUrl: '',
				isAnonymous: false
			},
			showGifts: false,
			isSubmitted: false,
			errors: {
				isValid: false,
				fullName: {
					isValid: false
				},
				giftName: {
					isValid: false
				}
			},
			isSuccess: false,
			allGifts: []
        },
        created: function() {
			this.$http.get('/api/getAllGiftRegistry').then(response => {
				this.allGifts = response.body.data || [];
				console.log(this.allGifts);
			});
        },
        methods: {
			registerGift: function () {
				this.validate();
				this.isSubmitted = true;
				if(this.errors.isValid) {
					this.$http.post('/api/getMetaItemsFromURL', this.form).then(response => {												
						this.allGifts.push({
							fullName: response.body.data && response.body.data.fullName ? response.body.data.fullName : '',
							giftName:  response.body.data && response.body.data.giftName ? response.body.data.giftName : '',
							imageUrl: response.body.data && response.body.data.imageUrl ? response.body.data.imageUrl : '',
							giftDescription: response.body.data && response.body.data.giftDescription ? response.body.data.giftDescription : ''
            });
            this.resetForm();
            this.isSuccess = true;
            var x = document.getElementById("snackbar");
            x.className = "show";
            setTimeout(function(){ x.className = x.className.replace("show", ""); }, 3000);
					}, response => {
					// error callback
					});
				}				
			},
			resetForm: function () {
				this.form.fullName = '';
				this.form.giftType = '';
				this.form.giftUrl = '';
				this.form.isAnonymous = '';
				this.isSubmitted = false;

			},
			validate: function () {
				let inValid = false;
				for(let item in this.errors) {
					if(item != 'isValid') {
						if(!this.form[item]) {
							this.errors[item].isValid = false;
							inValid = true;
						} else if(this.form[item]) {
							this.errors[item].isValid = true;							
						}
					}
				}
				this.errors.isValid = !inValid;
			}
        }
      });

    </script>
  </body>

</html>
