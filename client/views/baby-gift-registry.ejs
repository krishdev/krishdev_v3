	 font-weight: normal;
}
 @font-face {
	 font-family: "Grotesque Black";
	 src: url("https://s3-us-west-2.amazonaws.com/s.cdpn.io/397014/BrandonGrotesque-Black.eot"), url("https://s3-us-west-2.amazonaws.com/s.cdpn.io/397014/BrandonGrotesque-Black.ttf"), url("https://s3-us-west-2.amazonaws.com/s.cdpn.io/397014/BrandonGrotesque-Black.woff");
	 font-weight: bold;
}
 * {
	 box-sizing: border-box;
}
.c-Show-gifts {
  margin: 7em auto;
  max-width: 70%;
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  height: 100%;
  flex-flow: column nowrap;
}
.gifts-wrapper {
    position: relative;
    z-index: 11;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100%;
	flex-flow: row wrap;
}
 article.card {
	width: 32.33%;
    height: 340px;
    border-radius: 3px;
    box-shadow: 0 1px 4px rgba(0, 0, 0, .3);
    overflow: hidden;
	margin-bottom: 1em;
}
 article.card .thumb {
	 width: auto;
	 height: 260px;
	 background: url("/assets/images/gift-icon.png") no-repeat center;
	 background-size: cover;
	 border-radius: 3px;
}
 article.card .infos {
	 width: auto;
	 height: 350px;
	 position: relative;
	 padding: 14px 24px;
	 background: #fff;
	 transition: 0.4s 0.15s cubic-bezier(0.17, 0.67, 0.5, 1.03);
}
 article.card .infos .title {
	 position: relative;
